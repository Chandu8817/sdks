{
  "baseBranch": "origin/main",
  "pipeline": {
    "lint": {
      "inputs": [
        "sdks/*/src/**.ts",
        "sdks/*/src/**.tsx"
      ]
    },
    "build": {
      "inputs": [
        "yarn.lock",
        "**/package.json",
        "{sdks}/*/src/**.ts"
      ],
      "outputs": []
    },
    "test": {
      "dependsOn": [
        "build"
      ]
    },
    "release": {
      "dependsOn": [ ]
    },
    "typecheck": {
      "dependsOn": [ ]
    }
  }
}
